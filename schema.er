[follows]
    *id {label:"INTEGER"}
    created_at {label:"TIMESTAMP"}
    followed_id {label:"INTEGER"}
    follower_id {label:"INTEGER"}
[user_feed_association]
    *feed_id {label:"INTEGER"}
