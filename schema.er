[collection_articles]
    *id {label:"INTEGER"}
    added_at {label:"TIMESTAMP"}
    article_id {label:"INTEGER"}
    collection_id {label:"INTEGER"}
[collection_members]
    *id {label:"INTEGER"}
    collection_id {label:"INTEGER"}
    role {label:"VARCHAR(7)"}
    user_id {label:"INTEGER"}
[collection_permissions]
    *id {label:"INTEGER"}
    can_add_feed {label:"BOOLEAN"}
    can_comment {label:"BOOLEAN"}
    can_read {label:"BOOLEAN"}
    collection_id {label:"INTEGER"}
    user_id {label:"INTEGER"}
[notifications]
    *id {label:"INTEGER"}
    collection_id {label:"INTEGER"}
    comment_id {label:"INTEGER"}
    friendship_id {label:"INTEGER"}
    is_read {label:"BOOLEAN"}
