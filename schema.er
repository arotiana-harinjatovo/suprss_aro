[rss_feeds]
    *id {label:"INTEGER"}
    url {label:"VARCHAR"}
[rss_articles]
    *id {label:"INTEGER"}
    author {label:"VARCHAR"}
    feed_id {label:"INTEGER"}
    link {label:"VARCHAR"}
    published_at {label:"TIMESTAMP"}
    source_name {label:"VARCHAR"}
    summary {label:"VARCHAR"}
    tags {label:"JSON"}
    title {label:"VARCHAR"}
[users]
    *id {label:"INTEGER"}
    email {label:"VARCHAR"}
    full_name {label:"VARCHAR"}
    hashed_password {label:"VARCHAR"}
    is_active {label:"BOOLEAN"}
    username {label:"VARCHAR"}
[collections]
    *id {label:"INTEGER"}
    creator_id {label:"INTEGER"}
    description {label:"TEXT"}
    name {label:"VARCHAR"}
[feed_access]
    *id {label:"INTEGER"}
    can_delete {label:"BOOLEAN"}
    can_edit {label:"BOOLEAN"}
    feed_id {label:"INTEGER"}
    user_id {label:"INTEGER"}
[friendships]
    *id {label:"INTEGER"}
    created_at {label:"TIMESTAMP"}
    receiver_id {label:"INTEGER"}
    requester_id {label:"INTEGER"}
