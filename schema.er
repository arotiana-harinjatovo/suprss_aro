    receiver_id {label:"INTEGER"}
    requester_id {label:"INTEGER"}
    timestamp {label:"TIMESTAMP"}
    type {label:"VARCHAR"}
[comments]
    *id {label:"INTEGER"}
    article_id {label:"INTEGER"}
    created_at {label:"TIMESTAMP"}
    text {label:"VARCHAR"}
    user_id {label:"INTEGER"}
[user_article_association]
    *article_id {label:"INTEGER"}
    *user_id {label:"INTEGER"}
    is_favorite {label:"BOOLEAN"}
    is_read {label:"BOOLEAN"}
[collection_user_association]
    *collection_id {label:"INTEGER"}
    *user_id {label:"INTEGER"}
    is_active {label:"BOOLEAN"}
    role {label:"VARCHAR"}
[collection_messages]
    *id {label:"INTEGER"}
    collection_id {label:"INTEGER"}
    content {label:"TEXT"}
    timestamp {label:"TIMESTAMP"}
    user_id {label:"INTEGER"}
rss_feeds."id" 1--* rss_articles."feed_id"
